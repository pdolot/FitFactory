                            </group>
                            <path
                                    android:name="path"
                                    android:pathData="M 17 9 L 37.045 9 C 39.166 9 41.203 9.843 42.702 11.343 C 44.202 12.843 45.045 14.879 45.045 17 L 45.045 37.045 C 45.045 39.166 44.202 41.203 42.702 42.702 C 41.203 44.202 39.166 45.045 37.045 45.045 L 17 45.045 C 14.879 45.045 12.843 44.202 11.343 42.702 C 9.843 41.203 9 39.166 9 37.045 L 9 17 C 9 14.879 9.843 12.843 11.343 11.343 C 12.843 9.843 14.879 9 17 9"
                                    android:strokeColor="#4fb3bf"
