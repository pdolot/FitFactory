                            <group
                                    android:name="group"
                                    android:pivotX="27"
                                    android:pivotY="27">
                                <path
                                        android:name="path_3"
                                        android:pathData="M 27 3 C 20.637 3 14.529 5.53 10.029 10.029 C 5.53 14.529 3 20.637 3 27 C 3 33.363 5.53 39.471 10.029 43.971 C 14.529 48.47 20.637 51 27 51 C 33.363 51 39.471 48.47 43.971 43.971 C 48.47 39.471 51 33.363 51 27 C 51 20.637 48.47 14.529 43.971 10.029 C 39.471 5.53 33.363 3 27 3 Z"
